{
  "compilerOptions": {
    "baseUrl": "./",
  "paths": {
    "src/*": ["./src/*"]
  },
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "./dist",
    "skipLibCheck": true
  },
  

}
